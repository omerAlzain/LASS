<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_member_form" model="ir.ui.view">
        <field name="name">assl.member.form</field>
        <field name="model">assl.member</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="name"/>
                    <field name="date_adhesion"/>
                    <field name="email"/>
                    <field name="telephone"/>
                    <field name="genre"/>
                </group>
            </form>
        </field>
    </record>


    <record id="view_member_tree" model="ir.ui.view">
        <field name="name">assl.member.tree</field>
        <field name="model">assl.member</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="date_adhesion"/>
                <field name="email"/>
                <field name="telephone"/>
            </tree>
        </field>
    </record>

</odoo>